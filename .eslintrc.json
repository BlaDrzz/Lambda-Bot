{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "rules": {
        "semi": "error",
        "fp/no-arguments": "error",
        "fp/no-class": "error",
        "fp/no-delete": "error",
        "fp/no-events": "error",
        "fp/no-get-set": "error",
        "fp/no-let": "error",
        "fp/no-loops": "error",
        "fp/no-mutating-assign": ["error", {"commonjs": true}],
        "fp/no-mutating-methods": "error",
        "fp/no-mutation": ["error", {"commonjs": true}],
        "fp/no-proxy": "error",
        "fp/no-rest-parameters": "error",
        "fp/no-this": "error",
        "fp/no-throw": "error",
        "fp/no-valueof-field": "error",
        "no-var": "error",
        "no-empty": "warn",
        "no-unreachable": "error",
        "eqeqeq": "error"
    },
    "plugins": [
        "fp"
    ]
}
